    "express-session": "^1.17.3",
    "framer-motion": "^10.12.16",
    "handsontable": "^12.4.0",
    "ioredis": "^5.3.2",
    "is-mobile": "^4.0.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "lucide-react": "^0.244.0",
    "moment": "^2.29.4",
    "next": "^13.4.4",
    "next-redux-cookie-wrapper": "^2.2.1",
