{
  "name": "nest-next-app",
  "version": "0.0.1",
  "description": "nest-next-app",
  "author": "",
  "private": true,
  "repository": "https://github.com/Zoro-zqq/nest-next-app.git",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:next && yarn build:nest",
    "build:next": "next build",
    "build:nest": "nest build --path ./tsconfig.server.json --webpack --webpackPath=./webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "cross-env ts-node-dev --project tsconfig.server.json --ignore-watch .next --cls src/server/main.ts",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky install",
    "eslint": "eslint --max-warnings=0",
    "pre-check": "tsc && npx lint-staged",
    "postinstall": "prisma generate --schema=./src/server/prisma/schema.prisma",
    "stylelint": "stylelint",
    "seed": "node --loader ts-node/cjs ./src/server/prisma/seed.ts"
  },
  "lint-staged": {
    "src/**/*.{js,ts,json,css,scss,md}": [
      "prettier --config .prettierrc.js --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@neodrag/react": "2.0.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.4.2",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/websockets": "^9.4.2",
    "@onlyoffice/document-editor-react": "^1.3.0",
    "@prisma/client": "^4.15.0",
    "@react-three/drei": "8.7.4",
    "@react-three/fiber": "7.0.25",
    "@reduxjs/toolkit": "^1.9.5",
    "@rooks/use-raf": "^4.11.2",
    "ahooks": "^3.7.7",
    "antd": "^5.5.2",
    "axios": "^1.4.0",
    "axios-auth-refresh": "^3.3.6",
    "cache-manager": "^5.2.2",
